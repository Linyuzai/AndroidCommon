<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EasonIcon">

        <attr name="icon_percent_center" format="reference|float" />
        <attr name="icon_percent_center_x" format="reference|float" />
        <attr name="icon_percent_center_y" format="reference|float" />
        <attr name="icon_percent" format="reference|float" />
        <attr name="icon_percent_x" format="reference|float" />
        <attr name="icon_percent_y" format="reference|float" />
        <attr name="icon_percent_offset" format="reference|float" />
        <attr name="icon_percent_offset_x" format="reference|float" />
        <attr name="icon_percent_offset_y" format="reference|float" />
        <attr name="icon_offset" format="reference|dimension" />
        <attr name="icon_offset_x" format="reference|dimension" />
        <attr name="icon_offset_y" format="reference|dimension" />

        <attr name="icon_color" format="reference|color" />
        <attr name="icon_pen_size" format="reference|dimension" />
        <attr name="icon_pen_cap" format="enum">
            <enum name="butt" value="0" />
            <enum name="round" value="1" />
            <enum name="square" value="2" />
        </attr>
        <attr name="icon_pen_join" format="enum">
            <enum name="miter" value="0" />
            <enum name="round" value="1" />
            <enum name="bevel" value="2" />
        </attr>
        <attr name="icon_pen_style" format="enum">
            <enum name="fill" value="0" />
            <enum name="stroke" value="1" />
            <enum name="fill_stroke" value="2" />
        </attr>
        <attr name="icon_type" format="enum">
            <enum name="none" value="0" />
            <enum name="line_vertical" value="1" />
            <enum name="line_horizontal" value="2" />
            <enum name="line_slope_positive" value="3" />
            <enum name="line_slope_negative" value="4" />
            <enum name="circle" value="5" />
            <enum name="oval" value="6" />
            <enum name="rect" value="7" />
            <enum name="arc" value="8" />
            <enum name="bitmap" value="9" />
            <enum name="polygon" value="10" />
            <enum name="polygon_extra" value="11" />
            <enum name="polygon_quad" value="12" />
            <enum name="text" value="13" />
            <enum name="point" value="14" />

            <enum name="back" value="17" />
            <enum name="next" value="18" />
            <enum name="collapse" value="19" />
            <enum name="expand" value="20" />
            <enum name="back_solid_oval" value="21" />
            <enum name="next_solid_oval" value="22" />
            <enum name="collapse_solid_oval" value="23" />
            <enum name="expand_solid_oval" value="24" />
            <enum name="back_solid_rect" value="25" />
            <enum name="next_solid_rect" value="26" />
            <enum name="collapse_solid_rect" value="27" />
            <enum name="expand_solid_rect" value="28" />
            <enum name="arrow_left" value="29" />
            <enum name="arrow_right" value="30" />
            <enum name="arrow_up" value="31" />
            <enum name="arrow_down" value="32" />
            <enum name="arrow_left_solid_oval" value="33" />
            <enum name="arrow_right_solid_oval" value="34" />
            <enum name="arrow_up_solid_oval" value="35" />
            <enum name="arrow_down_solid_oval" value="36" />
            <enum name="arrow_left_solid_rect" value="37" />
            <enum name="arrow_right_solid_rect" value="38" />
            <enum name="arrow_up_solid_rect" value="39" />
            <enum name="arrow_down_solid_rect" value="40" />
            <enum name="add" value="41" />
            <enum name="minus" value="42" />
            <enum name="correct" value="43" />
            <enum name="error" value="44" />
            <enum name="add_solid_oval" value="45" />
            <enum name="minus_solid_oval" value="46" />
            <enum name="correct_solid_oval" value="47" />
            <enum name="error_solid_oval" value="48" />
            <enum name="add_solid_rect" value="49" />
            <enum name="minus_solid_rect" value="50" />
            <enum name="correct_solid_rect" value="51" />
            <enum name="error_solid_rect" value="52" />
            <enum name="setting" value="53" />
            <!-- api 21 <enum name="setting_v21" value="54" />-->
            <enum name="flower_extra" value="55" />
            <enum name="flower_quad" value="56" />
            <enum name="menu" value="57" />
            <enum name="user" value="58" />
            <enum name="male" value="59" />
            <enum name="female" value="60" />
            <enum name="sign" value="61" />
            <enum name="search" value="62" />
            <enum name="star" value="63" />
            <enum name="love" value="64" />
        </attr>
        <!-- 需要两种颜色 -->
        <attr name="icon_auxiliary_color" format="reference|color" />
        <!-- 需要缩放其中一个图形 -->
        <attr name="icon_auxiliary_scale" format="reference|float" />
        <!-- 需要圆角半径 -->
        <attr name="icon_round_all" format="reference|dimension" />
        <attr name="icon_round_left_top" format="reference|dimension" />
        <attr name="icon_round_left_bottom" format="reference|dimension" />
        <attr name="icon_round_right_top" format="reference|dimension" />
        <attr name="icon_round_right_bottom" format="reference|dimension" />
        <!-- 圆弧 -->
        <attr name="icon_arc_angle_start" format="reference|float" />
        <attr name="icon_arc_angle_sweep" format="reference|float" />
        <attr name="icon_arc_center" format="reference|boolean" />
        <!-- res -->
        <attr name="icon_image_res" format="reference" />
        <!-- 多边形 -->
        <attr name="icon_polygon_edge_count" format="reference|integer" />
        <attr name="icon_polygon_extra_offset" format="reference|dimension" />
        <attr name="icon_polygon_pen_size_scale" format="reference|float" />
        <!-- path -->
        <attr name="icon_path_effect_corner" format="reference|float" />
        <!-- text -->
        <attr name="icon_text" format="reference|string" />
        <attr name="icon_text_size" format="reference|dimension" />
    </declare-styleable>
    <declare-styleable name="RectImageView">
        <attr name="round_radius" format="reference|dimension" />
    </declare-styleable>
</resources>